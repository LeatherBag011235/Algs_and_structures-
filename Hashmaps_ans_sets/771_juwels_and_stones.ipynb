   "source": [
    "def numJewelsInStones( jewels: str, stones: str) -> int:\n",
    "    jewels_set = set(jewels)\n",
    "    ans = 0\n",
    "\n",
    "    for i in stones:\n",
    "        if i in jewels_set:\n",
    "            ans += 1\n",
    "\n",
    "    return ans"
   ]
